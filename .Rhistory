quick.subset <- function(x, domain){
x@data$id <- rownames(x@data)
x.f = fortify(x, region = "id")
x.join <- inner_join(x.f, x@data, by = "id")
x.subset <- subset(x.join, x.join$long > domain[1] &
x.join$long < domain[2] &
x.join$lat > domain[3] &
x.join$lat < domain[4])
x.subset
}
